{
    "builds": [
        {
          "src": "backend/manage.py",
          "use": "@vercel/python",
          "config": { "maxLambdaSize": "50mb", "runtime":"python3.9" }
        }
      ],
      "routes": [
        {
          "src": "/(.*)",
          "dest": "backend/manage.py"
        }
      ]
    
}